{
    "name": "lindritkrasniqi/avatar",
    "description": "Avatar package for any Laraval 8.x project.",
    "require-dev": {
        "orchestra/testbench": "^6.23"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lindrit Krasniqi",
            "email": "linndritks@gmail.com"
        }
    ],
    "require": {
        "intervention/image": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "lindritkrasniqi\\Avatar\\": "/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "lindritkrasniqi\\Avatar\\AvatarServiceProvider",
                "Intervention\\Image\\ImageServiceProvider"
            ],
            "aliases": {
                "Image": "Intervention\\Image\\Facades\\Image"
            }
        }
    }
}
